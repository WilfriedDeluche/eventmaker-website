{% capture html %}
<!DOCTYPE html>
<html lang="fr">
  <head>
    {% if page.seo_title != "" %}
      <title>{{ page.seo_title }}</title>
    {% else %}
      <title>{{ event.name }}</title>
    {% endif %}
    {{ csrf_meta_tag }}
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="{{page.seo_description}}">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {% assign baseurl = event.custom_domain_with_protocol %}
    <link rel="Canonical" href="{{baseurl}}/{{page.path_name | remove: "__id__"}}{{custom_entry.slug}}" />
    {% comment %}<link hreflang="fr" href="{{baseurl}}/{{page.path_name | remove: "__id__"}}{{custom_entry.slug}}" />{% endcomment %}
    <link rel="alternate" href="{{baseurl}}/{{page.path_name | remove: "__id__"}}{{custom_entry.slug}}" hreflang="x-default" />
    <link rel="alternate" hreflang="en" href="{{baseurl}}/en/{{page.path_name | remove: "__id__"}}{{custom_entry.slug}}" />
    <link rel="icon" type="image/png" href="https://applidget.github.io/vx-assets/orgs/applidget/eventmaker/voxevent/favicon.png" />
    <meta name="og:title"  content="{% if page.seo_title != "" %}<title>{{ page.seo_title }}</title>{% else %}<title>{{ event.name }}</title>{% endif %}"/>
    <meta name="og:type"  content="website"/>
    <meta name="og:url"  content="{{baseurl}}/{{page.path_name | remove: "__id__"}}{{custom_entry.slug}}"/>
    <meta name="og:site_name" content="Eventmaker"/>
    <meta name="og:description" content="{{page.seo_description}}"/>
    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="@Eventmakerio">
    <meta name="twitter:url" content="{{baseurl}}/{{page.path_name | remove: "__id__"}}{{custom_entry.slug}}">
    <meta name="twitter:title" content="{% if page.seo_title != "" %}<title>{{ page.seo_title }}</title>{% else %}<title>{{ event.name }}</title>{% endif %}">
    <meta name="twitter:description" content="{{page.seo_description}}">
    <!--[if IE]><link rel="shortcut icon" type="image/x-icon" href="https://applidget.github.io/vx-assets/orgs/applidget/eventmaker/voxevent/favicon.ico" /><![endif]-->
    <!-- BOOTSTRAP 4  -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/css/bootstrap.min.css" integrity="sha384-rwoIResjU2yc3z8GV/NPeZWAv56rSmLldC3R/AZzGRnGxQQKnKkoFVhFQhNUwEyJ" crossorigin="anonymous">
    <!-- Global Site Tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-20747305-6"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments)};
      gtag('js', new Date());
      gtag('config', 'UA-20747305-6');
    </script>
    <!-- plugins -->
    {{ 'bundle.min.css' | stylesheet_tag }}
    <!--main css file-->
    {{ 'style.css' | stylesheet_tag }}
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Editable generaux -->
    {% capture logo %}{% editable_file 'Logo menu', priority: 100 %}{% endeditable_file %}{% endcapture %}
    {% capture logo_scroll %}{% editable_file 'Logo menu au scroll', priority: 100 %}{% endeditable_file %}{% endcapture %}
    {% capture facebook %}{% editable_text "Lien Facebook", priority: 20 %}{% endeditable_text %}{% endcapture %}
    {% capture twitter %}{% editable_text "Lien Twitter", priority: 20 %}{% endeditable_text %}{% endcapture %}
    {% capture googleplus %}{% editable_text "Lien Google+", priority: 20 %}{% endeditable_text %}{% endcapture %}
    {% capture linkedin %}{% editable_text "Lien Linkedin", priority: 20 %}{% endeditable_text %}{% endcapture %}
    {% capture rss %}{% editable_text "Lien RSS", priority: 20 %}{% endeditable_text %}{% endcapture %}
    {% capture youtube %}{% editable_text "Lien YouTube", priority: 20 %}{% endeditable_text %}{% endcapture %}
    {% capture copyright %}{% editable_text "Copyright", priority: 15 %}&copy; Copyright 2016 - 2018 {% endeditable_text %}{% endcapture %}
    {% capture text_footer %}{% editable_text "Texte footer", priority: 25, type: cktext_area %}<p>Vous souhaitez une démo ? Un devis ? Contactez notre équipe commerciale au +33 (0)1 85 09 75 73 ou sur ecrire@eventmaker.io</p>{% endeditable_text %}{% endcapture %}
    {% capture text_cookie %}{% editable_text "Texte bannière Cookie ", priority: 15 %}Nous utilisons des cookies pour améliorer votre expérience {% endeditable_text %}{% endcapture %}
    {% capture text_lien_cookie %}{% editable_text "Texte Lien bannière Cookie ", priority: 15 %} en savoir plus {% endeditable_text %}{% endcapture %}
    {% capture lien_cookie %}{% editable_text "Lien bannière Cookie ", priority: 15 %} donnees-personnelles {% endeditable_text %}{% endcapture %}
  </head>
  <body>
    <div id="preloader">
      <div id="preloader-inner"></div>
    </div><!--/preloader-->
    <!--back to top-->
    <a href="#" class="scrollToTop"><i class="ion-ios-arrow-up"></i></a>
    <!--back to top end-->
    <!-- Static navbar -->
    <nav class="navbar navbar-toggleable-md navbar-light fixed-top  navbar-transparent clearfix">
      <div class="clearfix"></div>
      <div class="container">
        <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <a class="navbar-brand" href="/home">
          <img data-src="{{logo}}" alt="Eventmaker logo" class="logo logo-default">
          <img data-src="{{logo_scroll}}" alt="Eventmaker logo" class="logo logo-scroll">
        </a>
        <div class="collapse navbar-collapse" id="navbarNavDropdown">
          <ul class="navbar-nav ml-auto">
            {% for item in menus.menu_principal %}
              {% assign item_url = item.url | strip | remove: "/" %}
              {% assign page_path = page.path_name | strip %}
              <li class="nav-item">
                {% if item.name == 'Blog' %}
                  <a class="nav-item-mobil {% if page_path == item_url %}btn btn-primary btn-rounded{% endif %} nav-link" href="{{item.url}}" target="_blank" title="{{item.name}}">{{ item.name }}</a>
                {% elsif item.url == "/en/home" %}
                  <a id="en" class="nav-item-mobil {% if page_path == item_url %}btn btn-primary btn-rounded{% endif %} nav-link flag" href="#" title="{{item.name}}">{{ item.name }}</a>
                {% else %}
                  <a class="nav-item-mobil {% if page_path == item_url %}btn btn-primary btn-rounded{% endif %} nav-link" href="{{item.url}}" title="{{item.name}}">{{ item.name }}</a>
                {% endif %}
              </li>
            {% endfor %}
            <li class="nav-item">
            <a class="nav-item-mobil nav-link" href="{{baseurl}}/en/{{page.path_name | remove: "__id__"}}{{custom_entry.slug}}" title="English">EN</a>
            </li>
          </ul>
        </div>
      </div>
    </nav><!--navbar end-->
    {{ content }}
    <!-- </div> -->
    <div class="space-70"></div>
    <footer class="footer">
      <div class="space-50"></div>
      <div class="container">
        <div class="row vertical-align-child-top">
          <div class="col-md-5 margin-b-30">
            <div class="col-sm-12 col-md-10" style="padding:0;">{{text_footer}}</div>
            <a href="/home">
                <img data-src="{{logo}}" alt="Eventmaker logo" class="logo">
              </a>
          </div>
          <div class="col-md-2">
            <ul class="list-unstyled">
              {% for item in menus.menu_principal %}
              {% if forloop.index <= 4 %}
                <li><a href="{{item.url}}">{{ item.name }}</a></li>
              {% endif %}
              {% endfor %}
            </ul>
          </div>
          <div class="col-md-2">
            <ul class="list-unstyled">
              {% for item in menus.menu_principal %}
              {% if forloop.index > 4 %}
                <li><a href="{{item.url}}">{{ item.name }}</a></li>
              {% endif %}
              {% endfor %}
              <li><a href="{{baseurl}}/en" title="English">EN</a></li>
            </ul>
          </div>
          <div class="col-md-3">
            <ul class="list-inline social">
              {% if facebook != blank %}
                <li class="list-inline-item"><a href="{{facebook}}"><i class="ion-social-facebook"></i></a></li>
              {% endif %}
              {% if twitter != blank %}
                <li class="list-inline-item"><a href="{{twitter}}"><i class="ion-social-twitter"></i></a></li>
              {% endif %}
              {% if googleplus != blank %}
                <li class="list-inline-item"><a href="{{googleplus}}"><i class="ion-social-googleplus"></i></a></li>
              {% endif %}
              {% if linkedin != blank %}
                <li class="list-inline-item"><a href="{{linkedin}}"><i class="ion-social-linkedin"></i></a></li>
              {% endif %}
              {% if rss != blank %}
                <li class="list-inline-item"><a href="{{rss}}"><i class="ion-social-rss"></i></a></li>
              {% endif %}
              {% if youtube != blank %}
                <li class="list-inline-item"><a href="{{youtube}}"><i class="ion-social-youtube"></i></a></li>
              {% endif %}
            </ul>

          </div>
        </div>
        <div class="row col-md-12 sub-footer">
      <span>{{copyright}}</span>
      <div>
      {% for item in menus.menu_sub_footer %}
              <a href="{{item.url}}">{{ item.name }}</a>
      {% endfor %}
      </div>
      </div>
      </div>
      <div class="space-20"></div>
    </footer>
    <div class="cookie fixed-bottom">{{text_cookie}} <a href="{{baseurl}}/{{lien_cookie}}"><u>{{text_lien_cookie}}</u></a> <a class="btn btn-primary">OK</a></div>
